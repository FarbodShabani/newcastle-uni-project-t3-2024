<%- include('./partials/header'); %>
<h2 class="mt-3 mb-3 text-center">Please fill out the form</h2>
<!-- <div class="main-container"> -->
    <form>
    <div class=" mt-3 d-flex flex-row align-items-center justify-content-evenly flex-wrap align-content-center w-80 mx-auto">
           <% formElements.forEach(element => { %>
            
            <div class="mb-3 mx-1">
                <label for="<%= element.inputId %>" class="form-label"><%= element.name %></label>
                <input type="text" class="form-control" id="<%= element.inputId %>" />
            </div>
            
            <% }) %>
        </div>
        <div>
            
        </div>
        <button type="submit" class=" mx-auto btn btn-primary">Submit</button>
    </form>
            
            <!-- </div> -->
            <%- include('./partials/footer'); %>
            